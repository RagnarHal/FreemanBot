{
  "name": "freemanbot",
  "version": "0.0.1",
  "description": "A small bot for the HLF community Discord guild",
  "main": "main.js",
  "engines": {
    "node": "10.15.1"
  },
  "scripts": {
    "babel-node": "babel-node",
    "build": "rimraf dist/ && babel . --out-dir dist/ --ignore ./node_modules --ignore ./.heroku",
    "start": "yarn build && cross-env NODE_ENV=production node ./dist/main.js",
    "start:dev": "yarn build && cross-env NODE_ENV=development node ./dist/main.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec yarn babel-node -- ./main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src",
    "precommit": "eslint ./src"
  },
  "author": "Ragnar Mikael Halld√≥rsson",
  "license": "MIT",
  "dependencies": {
    "@google/maps": "^0.4.5",
    "axios": "^0.17.1",
    "common-tags": "^1.8.0",
    "discord.js": "^11.3.0",
    "discord.js-commando": "^0.10.0",
    "dotenv": "^4.0.0",
    "firebase-admin": "^7.0.0",
    "moment-timezone": "^0.5.14",
    "pg": "^7.4.1",
    "sherlockjs": "^1.3.7",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.16.0",
    "eslint-config-node": "^2.0.0",
    "husky": "^0.14.3",
    "nodemon": "^1.14.11"
  }
}
